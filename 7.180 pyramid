#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int row;
	cin>>row;
	
	for(int i=1;i<=row;i++){
	    for(int j=1;j<=row-i;j++){
	        cout<<" ";
	    }
	    for(int k=row-i+1;k<=row;k++){
	        cout<<"*";
	    }
	    cout<<endl;
	}
	
	return 0;
}


//output
    *
   **
  ***
 ****
*****
